<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace의 속성은 생성해논 interface의 이름과 동일하게 지정해줘야함-->
<mapper namespace="net.ict.springex.mapper.TimeMapper2">
<!--    select 태그는 반드시 resultType이나 resultMap을 써야줘야한다.
                왜냐 ResultSet 으로 결과 값을 받아야 하니까-->
    <select id="getNow" resultType="string">
        select now()
    </select>
</mapper>